    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <dct:title xml:lang="en">Sound recording: tape width</dct:title>
    <dct:alternative xml:lang="en">MARC21 007/07 values for sound recordings expressed using
