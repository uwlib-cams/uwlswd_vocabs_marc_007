  <rdf:Description rdf:about="https://doi.org/10.6069/uwlswd.fh0k-hk62">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <dct:title xml:lang="en">Sound recording: tape configuration</dct:title>
    <dct:alternative xml:lang="en">MARC21 007/08 values for sound recordings expressed using
         RDF</dct:alternative>
    <dct:description xml:lang="en">Number of tracks on a tape.</dct:description>
    <schema:disambiguatingDescription>SKOS Concept Scheme for MARC 00X
         Values</schema:disambiguatingDescription>
