  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#g",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Requires Dolby-C playback equipment."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "g"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Dolby-C encoded"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Used for tape recordings where the label or packaging clearly\n         indicates that Dolby-C is required. \"Dolby\" or the double-D symbol without specific\n         explanation normally indicates Dolby-B."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#c",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Requires Dolby-B decoding playback equipment."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "c"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Dolby-B encoded"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Indicated on commercial recordings by the double D insignia\n         (without indication of Dolby-C encoding). If Dolby is used on a tape recording without\n         other indication, Dolby-B may be assumed. Despite the presence of the double-D or \"Dolby\"\n         on a disc recording, this code is never used for commercial discs."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#n",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Item does not have special playback\n         characteristics."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "n"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "not applicable"
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#z",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Playback characteristics other than NAB standard, CCIR\n         standard, Dolby-B encoded, dbx encoded, digital recording, Dolby-A encoded, Dolby-C\n         encoded, or CX encoded."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "z"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "other"
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#e",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Requires digital playback equipment, such as Phillips/Sony\n         Compact Discs or Sony PCM tape recordings."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "e"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "digital recording"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Not used when there is indication that digital equipment was\n         used during recording, but that analog playback equipment may be used. No commercial\n         recordings prior to 1982, and no analog disc or tape recordings use digitial equipment\n         during recording."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#d",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Used for disc or tape recordings which indicate on the label or\n         package that dbx decoding is required."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "d"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "dbx encoded"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Not used if descriptive material indicates that dbx was used\n         during recording but that standard playback equipment may be used."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844#f",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Requires Dolby-A playback equipment."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.a3v4-v844"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "f"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Dolby-A encoded"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Used only for master tapes and other tape recordings where clear\n         indication is given that the professional Dolby-A encoding was applied, and that Dolby-A\n         decoding is required. Dolby-A encoding is never used for commercial tape or discs.\n         (Instantaneous tape recordings labelled \"Dolby\" without other indication are normally\n         Dolby-B, particularly cassette recordings.)"
      }
    ]
  }
]