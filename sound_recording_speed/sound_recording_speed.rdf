    <skos:notation xml:lang="en">l</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#r">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">30 ips</skos:prefLabel>
    <skos:definition xml:lang="en">30 inches per second.</skos:definition>
    <skos:scopeNote xml:lang="en">Occurs, for example, on reel-to-reel, studio tapes, but is
         rarely used.</skos:scopeNote>
    <skos:notation xml:lang="en">r</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#b">
