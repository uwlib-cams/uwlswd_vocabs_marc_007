    <skos:prefLabel xml:lang="en">33 1/3 rpm</skos:prefLabel>
    <skos:definition xml:lang="en">33 1/3 rpm.</skos:definition>
    <skos:scopeNote xml:lang="en">Used for discs.</skos:scopeNote>
    <skos:notation xml:lang="en">b</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#c">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">45 rpm</skos:prefLabel>
    <skos:definition xml:lang="en">45 rpm.</skos:definition>
    <skos:scopeNote xml:lang="en">Used for discs.</skos:scopeNote>
    <skos:notation xml:lang="en">c</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#z">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">other</skos:prefLabel>
    <skos:definition xml:lang="en">Speed other than 16 rpm, 33 1/3 rpm, 45 rpm, 78 rpm, 8 rpm, 1.4
         m. per second, 120 rpm, 160 rpm, 15/16 ips, 1 7/8 ips, 3 3/4 ips, 7 1/2 ips, 15 ips, or 30
         ips.</skos:definition>
    <skos:notation xml:lang="en">z</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#n">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
    <skos:definition xml:lang="en">Speed is not applicable to remote digital sound recordings
         because it pertains to calculations specific to physical aspects of
         carriers.</skos:definition>
    <skos:notation xml:lang="en">n</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#h">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">120 rpm</skos:prefLabel>
    <skos:definition xml:lang="en">120 rpm.</skos:definition>
    <skos:scopeNote xml:lang="en">Used for cylinders.</skos:scopeNote>
    <skos:notation xml:lang="en">h</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#o">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">7 1/2 ips</skos:prefLabel>
    <skos:definition xml:lang="en">7 1/2 inches per second.</skos:definition>
    <skos:scopeNote xml:lang="en">Occurs, for example, in reel-to-reel, nonprofessional, and
         studio tapes.</skos:scopeNote>
    <skos:notation xml:lang="en">o</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#i">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">160 rpm</skos:prefLabel>
    <skos:definition xml:lang="en">160 rpm.</skos:definition>
    <skos:scopeNote xml:lang="en">Used for cylinders.</skos:scopeNote>
    <skos:notation xml:lang="en">i</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">8 rpm</skos:prefLabel>
    <skos:definition xml:lang="en">8 rpm.</skos:definition>
    <skos:scopeNote xml:lang="en">Used for discs.</skos:scopeNote>
    <skos:notation xml:lang="en">e</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#m">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">3 3/4 ips</skos:prefLabel>
    <skos:definition xml:lang="en">3 3/4 inches per second.</skos:definition>
    <skos:scopeNote xml:lang="en">Occurs, for example, on consumer and nonprofessional
         reel-to-reel tapes, cartridges and very unusual cassette tapes.</skos:scopeNote>
    <skos:notation xml:lang="en">m</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#f">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">1.4 meters per second</skos:prefLabel>
    <skos:definition xml:lang="en">1.4 meters per second. Speed is calculated from the distance
         the playback mechanism covers on the surface of the disc per second, and not the number of
         revolutions of the disc.</skos:definition>
    <skos:scopeNote xml:lang="en">Associated with compact discs (CDs).</skos:scopeNote>
    <skos:notation xml:lang="en">f</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#a">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">16 rpm</skos:prefLabel>
    <skos:definition xml:lang="en">16 rpm.</skos:definition>
    <skos:scopeNote xml:lang="en">Used for discs.</skos:scopeNote>
    <skos:notation xml:lang="en">a</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#p">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
    <skos:prefLabel xml:lang="en">15 ips</skos:prefLabel>
    <skos:definition xml:lang="en">15 inches per second.</skos:definition>
    <skos:scopeNote xml:lang="en">Occurs, for example, on reel-to-reel, studio
         tapes.</skos:scopeNote>
    <skos:notation xml:lang="en">p</skos:notation>
  </rdf:Description>
