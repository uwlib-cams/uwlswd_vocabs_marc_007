  <rdf:Description rdf:about="http://fakeIRI.edu/terms/soundrecordingspd#l">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingspd"/>
