    skos:notation "m" ;
    skos:prefLabel "mixed"@en .

<https://doi.org/10.6069/uwlswd.x654-gd54#u> a skos:Concept ;
    skos:definition "Color of the microform is not known."@en ;
    skos:inScheme <https://doi.org/10.6069/uwlswd.x654-gd54> ;
