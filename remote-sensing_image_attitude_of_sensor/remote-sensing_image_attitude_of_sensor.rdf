    <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.06cw-h775"/>
    <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
    <skos:definition xml:lang="en">Angle of the remote-sensing device is not
         known.</skos:definition>
    <skos:notation>u</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlswd.06cw-h775#a">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.06cw-h775"/>
    <skos:prefLabel xml:lang="en">low oblique</skos:prefLabel>
    <skos:definition xml:lang="en">Remote-sensing device is closer to parallel with the surface
         being imaged than perpendicular.</skos:definition>
    <skos:notation>a</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlswd.06cw-h775#n">
