    <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.06cw-h775"/>
    <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
    <skos:definition xml:lang="en">Angle of the remote-sensing device is not applicable,
         generally, because the attitude does not affect the image.</skos:definition>
    <skos:notation>n</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlswd.06cw-h775#b">
