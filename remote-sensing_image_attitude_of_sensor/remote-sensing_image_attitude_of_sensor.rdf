    <skos:inScheme rdf:resource="https://doi.org/10.6069/uwlswd.06cw-h775"/>
    <skos:prefLabel xml:lang="en">high oblique</skos:prefLabel>
    <skos:definition xml:lang="en">Remote-sensing device is closer to perpendicular to the surface
