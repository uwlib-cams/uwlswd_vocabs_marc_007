      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Mass-produced cylinders are made of plastic. Early cylinders\n         were made of tinfoil or wax."
      }
    ]
  },
