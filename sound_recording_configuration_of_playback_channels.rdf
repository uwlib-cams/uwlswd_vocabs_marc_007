<!-- Scheme: MARC21-007: Configuration of playback channels of sound recordings -->
   <skos:ConceptScheme rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#">
      <dct:title>Sound recording: configuration of playback channels</dct:title>
      <dct:alternative>MARC21 007/04 values for sound recordings expressed using RDF</dct:alternative>
      <dct:description>Configuration of playback channels for a sound recording based on a clear indication of intended playback. Does not refer to the configuration of channels orginally recorded, unless those channels are all intended to be available on playback.</dct:description>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
   </skos:ConceptScheme>
   <!-- Concept: acoustic [obsolete]  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#a" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">acoustic [obsolete]</skos:prefLabel>
      <skos:notation xml:lang="en">a</skos:notation>
   </skos:Concept>
   <!-- Concept: monaural (digital) [obsolete]  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#f" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">monaural (digital) [obsolete]</skos:prefLabel>
      <skos:notation xml:lang="en">f</skos:notation>
   </skos:Concept>
   <!-- Concept: quadraphonic (digital) [obsolete]  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#g" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">quadraphonic (digital) [obsolete]</skos:prefLabel>
      <skos:notation xml:lang="en">g</skos:notation>
   </skos:Concept>
   <!-- Concept: stereophonic (digital) [obsolete]  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#j" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">stereophonic (digital) [obsolete]</skos:prefLabel>
      <skos:notation xml:lang="en">j</skos:notation>
   </skos:Concept>
   <!-- Concept: other (digital) [obsolete]  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#k" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">other (digital) [obsolete]</skos:prefLabel>
      <skos:notation xml:lang="en">k</skos:notation>
   </skos:Concept>
   <!-- Concept: monaural  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#m" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">monaural</skos:prefLabel>
      <rdfs:label xml:lang="en">monaural (electric)</rdfs:label>
      <skos:definition xml:lang="en">One channel.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingcpc#m"/>
   </skos:Concept>
   <!-- Concept: other  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#z" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Configuration other than monaural; quadraphonic, multichannel or surround; or stereophonic.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingcpc#z"/>
   </skos:Concept>
   <!-- Concept: quadraphonic  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#q" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">quadraphonic, multichannel or surround</skos:prefLabel>
      <rdfs:label xml:lang="en">quadraphonic</rdfs:label>
      <rdfs:label xml:lang="en">quadraphonic (electric)</rdfs:label>
      <skos:definition xml:lang="en">More than two separate channels.  Use for Dolby surround sound tracks.</skos:definition>
      <skos:notation xml:lang="en">q</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingcpc#q"/>
   </skos:Concept>
   <!-- Concept: stereophonic  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#s" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">stereophonic</skos:prefLabel>
      <rdfs:label xml:lang="en">stereophonic (electric)</rdfs:label>
      <skos:definition xml:lang="en">Two separate channels.</skos:definition>
      <skos:notation xml:lang="en">s</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingcpc#s"/>
   </skos:Concept>
   <!-- Concept: unknown  -->
   <skos:Concept rdf:about="http://fakeIRI.edu/terms/soundrecordingcpc#u" xml:lang="en">
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/soundrecordingcpc#"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Unknown.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
      <skos:exactMatch rdf:resource="http://marc21rdf.info/terms/soundrecordingcpc#u"/>
   </skos:Concept>
</rdf:RDF>
