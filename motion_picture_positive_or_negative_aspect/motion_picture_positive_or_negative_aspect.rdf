   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturepna">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
      <dct:title xml:lang="en">Motion picture: positive/negative aspect</dct:title>
      <dct:alternative xml:lang="en">MARC21 007/10 values for motion pictures expressed using
         RDF</dct:alternative>
      <dct:description xml:lang="en">Whether the film is positive or negative.</dct:description>
      <schema:disambiguatingDescription>SKOS Concept Scheme for MARC 00X
         Values</schema:disambiguatingDescription>
