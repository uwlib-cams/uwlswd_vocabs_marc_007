   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturepna#b">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna"/>
      <skos:prefLabel xml:lang="en">negative</skos:prefLabel>
      <skos:definition xml:lang="en">For a black-and-white film, tonal values are the opposite of
         those in the original subject matter. For a color film, tonal values are the complements of
         the original subject matter (e.g., red appears as green in a color
         negative).</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturepna#n">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Film does not have a positive/negative
         aspect.</skos:definition>
      <skos:notation xml:lang="en">n</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturepna#z">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Positive/negative aspect other than positive or
         negative.</skos:definition>
      <skos:notation xml:lang="en">z</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturepna#a">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna"/>
      <skos:prefLabel xml:lang="en">positive</skos:prefLabel>
      <skos:definition xml:lang="en">Colors and/or tonal values are the same as the original subject
         matter.</skos:definition>
      <skos:notation xml:lang="en">a</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturepna#u">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturepna"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Positive/negative aspect of the film is not
         known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
   </rdf:Description>
</rdf:RDF>
