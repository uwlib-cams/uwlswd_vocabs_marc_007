[
  {
    "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56#b",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Disc recordings captured using electrical equipment and stored\n         directly on a master surface."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "b"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "direct storage, not acoustical"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "All recordings made with microphones and other electrical\n         equipment prior to the availability of magnetic recording techniques in the late 1940s used\n         direct storage. Commercial recordings marked \"direct to disc\" or some equivalent phrase\n         also use this technique. Also known as electromechanical recording."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56#u",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Capture and storage technique are not known."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "u"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "unknown"
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56#a",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Usually a disc or cylinder recording, captured using an\n         acoustical horn and diaphragm and stored directly on a master surface."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "a"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "acoustical capture, direct storage"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Most acoustical recordings date from before 1927/29 when\n         electrical recording equipment became available."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56#d",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Sound recordings which were captured electrically and stored\n         using digital techniques."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "d"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "digital storage"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Such recordings, which became available in the 1980s, are\n         normally identified as \"digitally recorded\" or some similar phrase on the label or package.\n         \"Digital remaster\" or \"digital mixing\" does not imply original digital storage. Note that\n         Digital storage and digital playback should not be confused."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56#e",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Sound recordings which were captured using electrical\n         techniques and stored as modulations and pulses on a magnetic surface."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.v4qn-rb56"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "e"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "analog electrical storage"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Most recordings made from the late 1940s until early 1980s are\n         analog electrical recordings."
      }
    ]
  },
