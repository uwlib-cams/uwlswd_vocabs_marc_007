   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
      <dct:title xml:lang="en">Motion picture: color</dct:title>
      <dct:alternative xml:lang="en">MARC21 007/03 values for motion pictures expressed using
         RDF</dct:alternative>
      <dct:description xml:lang="en">Color characteristics of a motion picture.</dct:description>
      <schema:disambiguatingDescription>SKOS Concept Scheme for MARC 00X
         Values</schema:disambiguatingDescription>
      <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
      <dct:source rdf:resource="https://www.loc.gov/marc/bibliographic/bd007.html"/>
      <dct:source rdf:resource="http://marc21rdf.info"/>
      <dct:provenance rdf:resource="https://doi.org/10.6069/uwlswd.2e2b-y833"/>
      <dct:creator rdf:resource="http://viaf.org/viaf/139541794"/>
      <dct:contributor rdf:resource="http://viaf.org/viaf/151962300"/>
      <dct:issued>2024</dct:issued>
      <dc:contributor xml:lang="en">Metadata Management Associates</dc:contributor>
      <dc:language>en</dc:language>
      <dct:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0"/>
      <schema:version>1-0-0</schema:version>
      <dct:issued>2024</dct:issued>
      <dct:format rdf:resource="http://www.w3.org/ns/formats/RDF_XML"/>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#b">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">black-and-white</skos:prefLabel>
      <skos:definition xml:lang="en">Printed or executed in black-and-white.</skos:definition>
      <skos:notation xml:lang="en">b</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#h">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">hand colored</skos:prefLabel>
      <skos:definition xml:lang="en">Motion picture, produced by a printing or photographic process,
         is hand colored.</skos:definition>
      <skos:scopeNote xml:lang="en">Rarely used with commercial films since these are not typically
         issued with hand coloring.</skos:scopeNote>
      <skos:notation xml:lang="en">h</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#m">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">mixed</skos:prefLabel>
      <skos:definition xml:lang="en">Work or collection is a combination of black-and-white,
         multicolored, hand colored, and/or other images.</skos:definition>
      <skos:notation xml:lang="en">m</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#c">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">multicolored</skos:prefLabel>
      <skos:definition xml:lang="en">Printed or executed in more than one color.</skos:definition>
      <skos:scopeNote xml:lang="en">Used for color photographic processes.</skos:scopeNote>
      <skos:notation xml:lang="en">c</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#n">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
      <skos:definition xml:lang="en">Color characteristics are not applicable because the item has
         no images.</skos:definition>
      <skos:scopeNote xml:lang="en">Used, for example, when the item in hand is sound track film
         intended to accompany visual images not actually present.</skos:scopeNote>
      <skos:notation xml:lang="en">n</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#z">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">other</skos:prefLabel>
      <skos:definition xml:lang="en">Color other than black-and-white, multicolored, hand colored,
         or mixed.</skos:definition>
      <skos:scopeNote xml:lang="en">E.g., toned, stained, tinted, etc.</skos:scopeNote>
      <skos:notation xml:lang="en">z</skos:notation>
   </rdf:Description>

   <rdf:Description rdf:about="http://fakeIRI.edu/terms/motionpicturecol#u">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/motionpicturecol"/>
      <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
      <skos:definition xml:lang="en">Color characteristics of the motion picture are not
         known.</skos:definition>
      <skos:notation xml:lang="en">u</skos:notation>
   </rdf:Description>
</rdf:RDF>
