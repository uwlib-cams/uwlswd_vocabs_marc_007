[
  {
    "@id": "https://doi.org/10.6069/uwlswd.exa1-z360#b",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Consists of a strip of photographic film wound onto a single\n         core (or hub), housed in a protective container. Microfilm cartridges require reading\n         equipment (readers and reader-printers) that is specifically adapted for their use.\n         Normally, the end of the film is left free for threading into such equipment. Common film\n         lengths, widths, and reduction ratios are the same as those of microfilm\n         reels."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.exa1-z360"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "b"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "microfilm cartridge"
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.exa1-z360#g",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Sheet of opaque material bearing a number of microimages\n         arranged in a two-dimensional array. Microopaques resemble microfiche. Common dimensions\n         are 3x5 in. and 6x9 in. Common reduction ratios are approximately 24:1 or 20:1.\n         Microopaques usually have identifying information in text that is large enough to read\n         without magnification."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.exa1-z360"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "g"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "microopaque"
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.exa1-z360#z",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Specific material designation other than aperture card,\n         microfilm cartridge, microfilm cassette, microfilm reel, microfiche, microfiche cassette,\n         microopaque, microfilm slip, microfilm roll."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.exa1-z360"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "z"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "other"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Includes microchip (a sheet of photographic film of very small\n         dimensions), microdot (a minuscule piece of photgraphic film with ultrahigh reduction\n         ration), microform scroll (a roll of unusually wide photographic film)."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.exa1-z360#a",
    "@type": [
      "http://www.w3.org/2004/02/skos/core#Concept"
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Card with one or more rectangular opening(s), or aperture(s),\n         specifically prepared for the mounting or insertion of a piece of photographic film\n         containing one or more microimage(s). The cards are usually EAM (Electrical Accounting\n         Machine) punched cards, commonly known as IBM cards, that are standardized for use in\n         card-handling machines. Such cards contain only one aperture and most frequently have only\n         one microimage."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.exa1-z360"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "a"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "aperture card"
      }
    ]
  },
