<rdf:RDF
   xmlns:dc="http://purl.org/dc/elements/1.1/"
   xmlns:dct="http://purl.org/dc/terms/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:schema="https://schema.org/"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
    <dct:type rdf:resource="http://purl.org/dc/dcmitype/Dataset"/>
    <dct:title xml:lang="en">Remote-sensing image: platform use category</dct:title>
    <dct:alternative xml:lang="en">MARC21 007/07 values for remote-sensing images expressed using
         RDF</dct:alternative>
    <dct:description xml:lang="en">Primary use intended for the platform on which the sensor is
         located.</dct:description>
    <schema:disambiguatingDescription>SKOS Concept Scheme for MARC 00X
         Values</schema:disambiguatingDescription>
    <dct:publisher rdf:resource="http://viaf.org/viaf/139541794"/>
    <dct:source rdf:resource="https://www.loc.gov/marc/bibliographic/bd007.html"/>
    <dct:source rdf:resource="http://marc21rdf.info"/>
    <dct:provenance rdf:resource="https://doi.org/10.6069/uwlswd.2e2b-y833"/>
    <dct:creator rdf:resource="http://viaf.org/viaf/139541794"/>
    <dct:contributor rdf:resource="http://viaf.org/viaf/151962300"/>
    <dct:issued>2024</dct:issued>
    <dc:contributor xml:lang="en">Metadata Management Associates</dc:contributor>
    <dc:language>en</dc:language>
    <dct:license rdf:resource="http://creativecommons.org/publicdomain/zero/1.0"/>
    <schema:version>1-0-0</schema:version>
    <dct:format rdf:resource="http://www.w3.org/ns/formats/RDF_XML"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#n">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">not applicable</skos:prefLabel>
    <skos:definition xml:lang="en">Identification of platform use category is not
         applicable.</skos:definition>
    <skos:notation xml:lang="en">n</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#z">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">other</skos:prefLabel>
    <skos:definition xml:lang="en">Platform use category other than meteorological, surface
         observing, space observing, or mixed uses.</skos:definition>
    <skos:notation xml:lang="en">z</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#a">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">meteorological</skos:prefLabel>
    <skos:definition xml:lang="en">Primary use for the platform is to make remote-sensing images
         of meteorological events and conditions.</skos:definition>
    <skos:notation xml:lang="en">a</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#b">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">surface observing</skos:prefLabel>
    <skos:definition xml:lang="en">Primary use for the platform is to make remote-sensing images
         of the surface of a planet, moon, etc. (including the Earth).</skos:definition>
    <skos:notation xml:lang="en">b</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#m">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">mixed uses</skos:prefLabel>
    <skos:definition xml:lang="en">Platform from which remote-sensing images can be made is
         designed for a variety of uses.</skos:definition>
    <skos:notation xml:lang="en">m</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#c">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">space observing</skos:prefLabel>
    <skos:definition xml:lang="en">Primary use for the platform is to make remote-sensing images
         of space.</skos:definition>
    <skos:notation xml:lang="en">c</skos:notation>
  </rdf:Description>
  <rdf:Description rdf:about="http://fakeIRI.edu/terms/remotesensingpla#u">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:inScheme rdf:resource="http://fakeIRI.edu/terms/remotesensingpla"/>
    <skos:prefLabel xml:lang="en">unknown</skos:prefLabel>
    <skos:definition xml:lang="en">Platform use category is not known.</skos:definition>
    <skos:notation xml:lang="en">u</skos:notation>
  </rdf:Description>
</rdf:RDF>
