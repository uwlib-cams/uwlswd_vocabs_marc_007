    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "nitrate base"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Cellulose nitrate support or base was used in the manufacture of\n         35 mm. film (and some 17.5 mm. film) until 1951. Nitrate base film is no longer\n         manufactured."
      }
    ]
  },
