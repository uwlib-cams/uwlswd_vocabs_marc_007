    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Cellulose diacetate film base."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.fqx4-1d72"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#notation": [
      {
        "@value": "d"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "safety base, diacetate"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Introduced before World War I for home movies, diacetate base\n         was more expensive and unpredictable than nitrate base and so failed to gain acceptance in\n         professional 35 mm. film production."
      }
    ]
  },
