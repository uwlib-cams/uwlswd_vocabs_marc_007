    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "safety base, triacetate"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#scopeNote": [
      {
        "@language": "en",
        "@value": "Cellulose triacetate is a high acetal compound with very low\n         flammability and slow burning characteristics. From 1951, triacetate has been used for\n         professional as well as for amateur produced moving image film."
      }
    ]
  },
  {
    "@id": "https://doi.org/10.6069/uwlswd.fqx4-1d72#d",
