    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Cellulose triacetate film base."
      }
    ],
    "http://www.w3.org/2004/02/skos/core#inScheme": [
      {
        "@id": "https://doi.org/10.6069/uwlswd.fqx4-1d72"
